<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="Application.WizardTemplate.controller.Test"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<Wizard id="CreateProductWizard" complete="wizardCompletedHandler" showNextButton="true" enableBranching="false">
						<WizardStep id="noticreated" title="Notification Created" icon="sap-icon://customer" nextStep="pmoassigned" complete="step1complt"
							validated="false"/>
						<WizardStep id="pmoassigned" title="PMO Assigned" icon="sap-icon://customer" nextStep="AddressInfoStep" complete="step1complt"
							validated="false"/>
					</Wizard>
				</content>
				<footer>
					<OverflowToolbar>
						<Button text="Next" press="onNext"/>
					</OverflowToolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>